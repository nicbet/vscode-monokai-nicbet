{
    "name": "Monokai Nicbet",
    "type": "dark",
    "colors": {
        "editor.background": "#181920",
        "editor.foreground": "#ffffff",
        "editor.lineHighlightBackground": "#2F2F34",
        "editorWhitespace.foreground": "#424242",
        "editorIndentGuide.background": "#616161",
        "editor.findMatchHighlightBackground": "#DD2C00",
        "tree.indentGuidesStroke": "#FFFFFF80",
        // Activity Bar
        "activityBar.background": "#0D0F12",
        // Side bar
        "sideBar.background": "#121519",
        "sideBar.foreground": "#9CA0B4",
        "sideBar.border": "#666666",
        // Gutter
        "editorGutter.modifiedBackground": "#8C550D",
        "editorGutter.addedBackground": "#165C3C",
        "editorGutter.deletedBackground": "#790F3F",
        "editorGutter.background": "#1D1E26",
        // Editor - Indentation Guides
        // "editorIndentGuide.background": "#333333",
        // "editorIndentGuide.activeBackground": "#FDCA0E",
        // Status Bar
        "statusBar.background": "#0F1014",
        "statusBarItem.prominentBackground": "#FF006B",
        // Title Bar
        "titleBar.activeBackground": "#0F1014",
        "titleBar.activeForeground": "#DDDDDD",
        "titleBar.inactiveBackground": "#0F1014",
        "titleBar.inactiveForeground": "#999999",
        // Editor Groups and Tabs
        "editorGroup.background": "#0F1014",
        "editorGroupHeader.noTabsBackground": "#0F1014",
        "editorGroupHeader.tabsBackground": "#0F1014",
        "tab.activeBackground": "#16171D",
        "tab.activeForeground": "#D6D8E0",
        "tab.activeBorder": "#5B71F6",
        "tab.inactiveBackground": "#0F1014",
        "tab.inactiveForeground": "#646674",
        // Matches
        "editor.selectionBackground": "#515049",
        "editor.selectionHighlightBorder": "#FFFFFF",
        "editor.selectionHighlightBackground": "#FFFFFF00",
        // Editor - Overview Ruler
        "editorOverviewRuler.modifiedForeground": "#181920",
        "editorOverviewRuler.addedForeground": "#181920",
        "editorOverviewRuler.deletedForeground": "#181920",
        // VSCode Remote icon
        "statusBarItem.remoteBackground": "#121519",
        "statusBarItem.remoteForeground": "#ccc"
    },

    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#75715E",
                "background": "#75715E"
            }
        },
        {
          "name": "Editor Content",
          "scope": [
            "punctuation.separator",
            "punctuation.accessor",
            "punctuation.definition.array"
          ],
          "settings": {
            "foreground": "#FFFF"
          }
        },
        {
            "name": "Keywords, Punctuation",
            "scope": [
                "keyword",
                "keyword.control",
                "storage.type",
                "storage.modifier",
                "punctuation.definition.parameters",
                "punctuation.section.parens.begin",
                "variable.other.property",
                "support.variable.property.dom",
                "variable.other.object.property",
                "entity.name.tag.css",
                "entity.name.tag.html",
                "entity.name.tag.js",
                "entity.name.tag.js.jsx",
                "punctuation.section.embedded.begin.erb",
                "punctuation.section.embedded.end.erb"
            ],
            "settings": {
                "foreground": "#FF006B"
            }
        },
        {
            "name": "Function Parameters",
            "scope": [
                "variable.parameter",
            ],
            "settings": {
                "foreground": "#F6AA11",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Objects",
            "scope": [
                "variable.other.object",
                "support.class.console",
                "support.class.ruby",
                "support.variable.dom",
                "support.constant",
                "entity.name.type.module",
                "entity.name.type",
                "support.function.js",
            ],
            "settings": {
                "foreground": "#30F5FF"
            }
        },
        {
          "name": "Italic Support",
          "scope": [
            "entity.other.inherited-class.ts",
            "meta.property-name.scss",
            "support.type.property-name.css",
            "support.class.component.js",
            "support.class.component.js.jsx"
          ],
          "settings": {
              "foreground": "#30F5FF",
              "fontStyle": "italic"
          }
        },
        {
            "name": "Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#A372FF"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "constant.other.symbol"
            ],
            "settings": {
                "foreground": "#FFED83"
            }
        },
        {
            "name": "Quotes",
            "scope": [
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end"
            ],
            "settings": {
                "foreground": "#FFED83"
            }
        },
        {
            "name": "Hash Keys",
            "scope": [
                "constant.language.symbol.hashkey.ruby",
                "meta.object-literal.key.js",
                "meta.object-literal.key.js.jsx",
                "punctuation.separator.key-value.js",
                "punctuation.separator.key-value.js.jsx"
            ],
            "settings": {
                "foreground": "#A372FF",
                "fontStyle": "underline",
            }
        },
        {
            "name": "Embedded Ruby",
            "scope": [
                "punctuation.section.embedded.begin.ruby",
                "punctuation.section.embedded.end.ruby",
                "punctuation.section.embedded.begin.js",
                "punctuation.section.embedded.begin.js.jsx",
                "punctuation.section.embedded.end.js",
                "punctuation.section.embedded.end.js.jsx"
            ],
            "settings": {
                "foreground": "#30F5FF",
            }
        },
        {
            "name": "Method definitions",
            "scope": [
                "entity.name.function",
                "support.function.dom",
                "support.function",
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.html",
                "entity.other.attribute-name.js",
                "entity.other.attribute-name.js.jsx",
                "meta.attribute.lang.html",
                "keyword.other.special-method.ruby"
            ],
            "settings": {
                "foreground": "#95FF17",
                "fontStyle": "normal"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable.language",
                "variable.other.member",
                "variable.other.readwrite",
                "punctuation.definition.variable",
                "meta.function.method.with-arguments.ruby",
                "variable.other.block.ruby",
                "meta.block.parameters.ruby",
                "meta.definition.property.ts",
                "entity.name.type.module.ruby"
            ],
            "settings": {
                "foreground": "#F6AA11"
            }
        }
    ]
}
